states {
    shadingModel : unlit,
    depthWrite : true,
    blendingMode : transparent
}

requires {
    uv0
}

samplers {
    sampler2D albedo_texture
}

fragment {
    void materialFragment(inout MaterialFragmentInputs material) {
        material.base_color = texture(albedo_texture, vertex_uv01.xy);
    }
}